<?xml version='1.0'?>
<txnmgr name="ACQ-TXNMGR" class="org.jpos.transaction.TransactionManager"  realm="acquiring" logger="Q2">
    <property name="queue" value="ACQ-TXNMGR"/>
    <property name="sessions" value="10"/>
    <property name="max-sessions" value="128"/>
    <property name="debug" value="true"/>
    <property name="debug-context" value="true"/>

    <participant class="com.cannyquest.participants.acquiring.prepareAcquiringContext" logger="Q2"/>

    <participant class="com.cannyquest.participants.acquiring.Switch" logger="Q2">
        <property name="500000" value="BillPayment" />
        <property name="000000" value="RETAIL" />
        <property name="010000" value="CASH" />
    </participant>

    <group name="RETAIL">
        <participant class="com.cannyquest.participants.acquiring.retail.BlockMCC" logger="Q2">
            <property name="blocked-mcc" value="6010"/>
            <property name="blocked-mcc" value="6011"/>
        </participant>
        <participant class="com.cannyquest.participants.acquiring.retail.BlockCashExpress" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.retail.MockAcquirerID" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.common.transformDHItoSVFE" logger="Q2"/>
        <participant class="org.jpos.transaction.participant.QueryHost" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.common.transfromSVFEtoDHI" logger="Q2"/>
        <participant class="org.jpos.transaction.participant.SendResponse" logger="Q2"/>
    </group>

    <group name="CASHEXPRESS">
        <participant class="com.cannyquest.participants.acquiring.cashexpress.prepareCashExpressContext" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.cashexpress.MockAcquirerID" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.cashexpress.prepareBalanceInquiry" logger="Q2"/>
        <participant class="org.jpos.transaction.participant.QueryHost" logger="Q2">
            <property name="request" value="CASH_EXPRESS_BALANCE_INQUIRY_REQUEST"/>
            <property name="response" value="CASH_EXPRESS_BALANCE_INQUIRY_RESPONSE"/>
        </participant>
        <participant class="com.cannyquest.participants.acquiring.cashexpress.prepareCashWithdrawalContext" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.cashexpress.prepareCashWithdrawal" logger="Q2"/>
        <participant class="org.jpos.transaction.participant.QueryHost" logger="Q2">
            <property name="request" value="CASH_EXPRESS_CASH_WITHDRAWAL_REQUEST"/>
            <property name="response" value="CASH_EXPRESS_CASH_WITHDRAWAL_RESPONSE"/>
        </participant>
        <participant class="com.cannyquest.participants.acquiring.cashexpress.prepareCashExpressResponseContext" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.cashexpress.prepareCashExpressResponse" logger="Q2">
            <property name="response" value="CASH_EXPRESS_CASH_WITHDRAWAL_RESPONSE"/>
        </participant>
        <participant class="org.jpos.transaction.participant.SendResponse" logger="Q2">
            <property name="response" value="CASH_EXPRESS_RESPONSE"/>
        </participant>
    </group>

    <group name="CASH">
        <participant class="com.cannyquest.participants.acquiring.cash.BlockMCC" logger="Q2">
            <property name="blocked-mcc" value="6010"/>
            <property name="blocked-mcc" value="6011"/>
        </participant>
        <participant class="com.cannyquest.participants.acquiring.cash.BlockCashExpress" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.cash.MockAcquirerID" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.common.transformDHItoSVFE" logger="Q2"/>
        <participant class="org.jpos.transaction.participant.QueryHost" logger="Q2"/>
        <participant class="com.cannyquest.participants.acquiring.common.transfromSVFEtoDHI" logger="Q2"/>
        <participant class="org.jpos.transaction.participant.SendResponse" logger="Q2"/>
    </group>
</txnmgr>